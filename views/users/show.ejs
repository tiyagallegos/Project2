<!----Show patient's med info, progress notes-->
<h1><%= p.name %>'s Feeding Profile</h1>

<table id="list">
    <thead>
        <tr>
          <th>Name</th>
          <th>Age</th>
          <th>Diagnoses</th>
        </tr>
      </thead>
      <tbody>
          <% user.forEach(function(p) {%>
          <tr>
            <td><%= p.name %></td>
            <td><%= p.age %></td>
            <td><%= p.birthdate %></td>
            <td><%= p.allergies %></td>
            <td><%= p.diagnoses %></td>
            <td><%= p.history %></td>
            <td><%= p.concerns %></td>
          </tr>
          <% }); %>
        </tbody>
      </table>

      <label> Progress Notes</label>
      <% user.forEach(function(note) {%>
      <ul>
          <li><%= note.date %></li>
          <a  id="details-button" href="/notes/<%= note._id %>">View Full Note</a>
      </ul>
      
      <a href="/users/index">Back to User Profile</a>